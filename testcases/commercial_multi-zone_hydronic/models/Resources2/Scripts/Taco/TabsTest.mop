optimization TabsTest(objectiveIntegrand = (hea.Q_flow-coo.Q_flow+costViol)/1e11, startTime=0, finalTime=1e6)
  input Real[2] slack(each free=true, each start=0, each min=0, each nominal = 1);
  extends INFRAX.MPC.TabsTest(enableGitLogger=false, y_cca2(min=0,max=1,start=1,free=true));
  output Real costViol=1e5*sum(slack); 

  output Real QHea = hea.Q_flow;
  output Real QCoo = coo.Q_flow;
 
constraint
heatCapacitor.port.T>=ones(35)*290;

end TabsTest;


